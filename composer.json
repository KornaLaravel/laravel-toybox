{
    "name": "nikspyratos/toybox-laravel",
    "type": "project",
    "description": "Laravel micro-SaaS all-in-one boilerplate",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "filament/filament": "^3.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/folio": "^1.0",
        "laravel/framework": "^11.0",
        "laravel/octane": "^2.3",
        "laravel/pulse": "dev-validation-errors",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.4",
        "livewire/volt": "^1.0",
        "pboivin/filament-peek": "^2.2",
        "pxlrbt/filament-environment-indicator": "^2.0",
        "spatie/laravel-activitylog": "^4.8",
        "spiral/roadrunner-cli": "^2.6",
        "spiral/roadrunner-http": "^3.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.10",
        "barryvdh/laravel-ide-helper": "^3.0",
        "enlightn/enlightn": "dev-l11-compatibility",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^2.8",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "laravel/telescope": "^5.0",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "nunomaduro/phpinsights": "^2.8",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "pestphp/pest-plugin-type-coverage": "^2.8",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.4",
        "tightenco/duster": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade",
            "@php artisan ide-helper:eloquent -n",
            "@php artisan ide-helper:meta -n",
            "@php artisan ide-helper:generate -n",
            "@php artisan ide-helper:models -n --reset"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "analysis": [
            "./vendor/bin/phpstan analyse",
            "@php artisan insights --fix -v -n",
            "@php artisan enlightn"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/enlightn.git"
        }
    ]
}
